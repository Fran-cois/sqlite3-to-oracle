# Exemple de fichier .env pour sqlite3-to-oracle
# Copiez ce fichier vers .env et ajustez les valeurs selon vos besoins

# Configuration de connexion Oracle
ORACLE_ADMIN_USER=system
ORACLE_ADMIN_PASSWORD=YourPassword
ORACLE_ADMIN_DSN=localhost:1521/XEPDB1

# Options générales
#ORACLE_SQLITE_DB=/chemin/vers/votre_base.sqlite
#ORACLE_NEW_USERNAME=mon_utilisateur
#ORACLE_NEW_PASSWORD=mon_mot_de_passe
#ORACLE_OUTPUT_FILE=/chemin/vers/sortie.sql

# Options d'importation
#ORACLE_DROP_TABLES=true
#ORACLE_FORCE_RECREATE=true
#ORACLE_SCHEMA_ONLY=false
#ORACLE_USE_VARCHAR=true
#ORACLE_ONLY_FK_KEYS=false

# Options pour le traitement par lots
#ORACLE_BATCH=false
#ORACLE_SQLITE_DIR=/chemin/vers/repertoire
#ORACLE_FILE_PATTERN=*.sqlite
#ORACLE_URI_OUTPUT_FILE=uris.txt
#ORACLE_CONTINUE_ON_ERROR=false
